{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ver diagnostico</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ diagnostico.id }}</td>
            </tr>
            <tr>
                <th>Diagnostico</th>
                <td>{{ diagnostico.diagnostico }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('diagnostico_index') }}">Volver a la lista</a>
        </li>
        <li>
            <a href="{{ path('diagnostico_edit', { 'id': diagnostico.id }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Eliminar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
